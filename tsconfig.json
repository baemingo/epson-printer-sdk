{
  "compilerOptions": {
    "lib": ["dom", "es5", "es6", "es2019", "es2020"],
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "declaration": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}
